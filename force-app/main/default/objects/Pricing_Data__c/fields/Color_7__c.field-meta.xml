				Max_Pieces__c &lt;= $Setup.OpptyProds__c.Price_Change_Quantity__c , 
				Color_1__c + ($Setup.OpptyProds__c.PricingData1__c) * 6, 
				Color_1__c + ($Setup.OpptyProds__c.PricingData2__c) * 6
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Color# 7</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
