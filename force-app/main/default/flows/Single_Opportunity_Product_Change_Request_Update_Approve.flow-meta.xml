    <formulas>
        <name>AddonorDNP</name>
        <dataType>String</dataType>
        <expression>IF(
    {!$Record.Quantity_Requested__c} &gt; {!$Record.Old_Quantity__c}, 
    &quot;Add-On&quot;, 
    IF(
        {!$Record.Quantity_Requested__c} &lt; {!$Record.Old_Quantity__c}, 
        &quot;Reduced&quot;, 
        IF(
            {!$Record.Quantity_Requested__c} = 0, 
            &quot;DNP&quot;, 
            &quot;Check!&quot;
        )
    )
)</expression>
    </formulas>
    <formulas>
        <name>NewStageNumber</name>
        <dataType>Number</dataType>
        <expression>{!$Record.Quantity_Requested__c}-{!$Record.Old_Quantity__c}</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>QuantityPlus1</name>
        <dataType>Number</dataType>
        <expression>{!$Record.Quantity_Requested__c}+1</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>WhichOppLocation</name>
