    <formulas>
        <name>CloseDatePlus12Months</name>
        <dataType>Date</dataType>
        <expression>ADDMONTHS(TODAY(), 12)</expression>
    </formulas>
    <formulas>
        <name>mustHave</name>
        <dataType>Date</dataType>
        <expression>ADDMONTHS({!$Record.Must_Have_Date__c},12)</expression>
    </formulas>
    <formulas>
        <name>nameFinal</name>
        <dataType>String</dataType>
        <expression>IF(LEN({!addWLY}) &gt; 100, LEFT({!addWLY}, 100), {!addWLY})</expression>
    </formulas>
    <formulas>
        <name>Today</name>
        <dataType>Date</dataType>
