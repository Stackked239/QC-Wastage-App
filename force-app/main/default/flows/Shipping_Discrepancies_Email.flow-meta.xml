    <decisions>
        <name>Cost_greater_than_billed</name>
        <label>Cost greater than billed</label>
        <locationX>172</locationX>
        <locationY>444</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Cost_greater</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Shipping_Cost__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>$Record.Billed_Shipping__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Ship_date_greater_than_prod</targetReference>
            </connector>
            <label>Cost greater</label>
        </rules>
    </decisions>
    <decisions>
        <name>Ship_date_greater_than_prod</name>
        <label>Ship date greater than prod</label>
        <locationX>419</locationX>
        <locationY>445</locationY>
        <defaultConnector>
            <targetReference>Email_with_Sales_at_fault</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Ship_date_grater</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Ship_Date__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>$Record.Auto_Production_Date__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Emailproductionatfault</targetReference>
            </connector>
            <label>Ship date grater</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>ShippingCostDifference</name>
