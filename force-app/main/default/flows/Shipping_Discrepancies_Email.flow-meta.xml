    <start>
        <locationX>50</locationX>
        <locationY>50</locationY>
        <connector>
            <targetReference>Cost_greater_than_billed</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Shipping_Cost__c</field>
            <operator>GreaterThan</operator>
            <value>
                <numberValue>0.0</numberValue>
            </value>
        </filters>
        <filters>
            <field>OPP_Owner__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Vanessa Mahai</stringValue>
            </value>
        </filters>
        <filters>
            <field>Billed_Shipping__c</field>
            <operator>GreaterThan</operator>
            <value>
                <numberValue>0.0</numberValue>
            </value>
        </filters>
        <filters>
            <field>Auto_Production_Date__c</field>
            <operator>GreaterThan</operator>
            <value>
                <dateValue>2022-01-01</dateValue>
            </value>
        </filters>
        <filters>
            <field>ShippingDiscrepancy__c</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <object>Opportunity</object>
        <recordTriggerType>Update</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
