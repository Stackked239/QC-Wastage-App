public with sharing class gcPurchaseOrder_receive {

    public class gcException extends Exception{}

    public class params{

        //these parameters may be passed in
        public String   action;
        public Id       locId;
        public Id       poId;
        public Id       poLineId;
        public String   productCode;
        public String   serialNumber;
        public Decimal  qty;

        //these parameters are not passed in
        public AcctSeedERP__Purchase_Order_Line__c poLine;
        public list<AcctSeedERP__Purchase_Order_Line__c> poLines;
        public list<AcctSeedERP__Purchase_Order_Inventory_Movement__c> poMoves;

        params(String strParams){
            map<String,Object> mapParams = new map<String,Object>();
            try{
                mapParams = (Map<String, Object>)JSON.deserializeUntyped(encodingUtil.urlDecode(strParams,'UTF-8'));
            }catch(exception e){}

            for(String key : mapParams.keySet()){
                String val;
                try{val = (String)mapParams.get(key);}catch(exception e){}
                if(val == null || val.isWhitespace())
                    continue;
                val = encodingUtil.urlDecode(val,'UTF-8'); 

                switch on key{
                    when 'action'               {this.action        = (String)val;}
                    when 'locId'                {this.locId         = (Id)val;}
                    when 'poId'                 {this.poId          = (Id)val;}                    
                    when 'poLineId'             {this.poLineId      = (Id)val;}
                    when 'productCode'          {this.productCode   = (String)val;}
                    when 'serialNumber'         {this.serialNumber  = (String)val;}
                    when 'qty'                  {this.qty           = Decimal.valueOf(val);}
                }
            }  
        }
    }

    @AuraEnabled
    public static String process(String strParams){
        try{
            params params = new params(strParams);
            process_local(params);
            return JSON.serialize(params);
        }
        catch(Exception e){
            throw new AuraHandledException(e.getMessage());
        }
    }

    public static void process_local(params params){
        
        if(params.action == 'selectPoLine'){

            retrievePoLine(params);

            params.poLines = retrievePoLines(params.poId);
            params.poMoves = retrievePoMoves(params.poLineId);

        }
        else if(params.action == 'createPoMove'){
            
            createPoMove(params);

            params.poLines = retrievePoLines(params.poId);
            params.poMoves = retrievePoMoves(params.poId);

        }
        else{
            throw new gcException('Invalid params.action');
        }
    }
    
    public static void retrievePoLine(params params){

        params.poLine = null;

        if(params.poLineId != null){
            for(AcctSeedERP__Purchase_Order_Line__c polx : [
                SELECT Id, 
                        Name,
                        AcctSeedERP__GL_Account_Variable_1__c,
                        AcctSeedERP__GL_Account_Variable_2__c,
                        AcctSeedERP__GL_Account_Variable_3__c,
                        AcctSeedERP__GL_Account_Variable_4__c,
                        AcctSeedERP__Product__c,
                        AcctSeedERP__Product__r.AcctSeed__Inventory_Product__c,
                        AcctSeedERP__Product__r.Case_Count__c,
                        AcctSeedERP__Project__c,
                        AcctSeedERP__Project_Task__c,
                        AcctSeedERP__Purchase_Order__c,
                        AcctSeedERP__Quantity__c,
                        AcctSeedERP__Quantity_Received__c,
                        AcctSeedERP__Quantity_Unreceived__c,
                        AcctSeedERP__Total__c,
                        AcctSeedERP__Unit_Price__c 
                FROM AcctSeedERP__Purchase_Order_Line__c 
                WHERE Id = :params.poLineId
                LIMIT 1
            ]){
                params.poLine = polx;
                return;
            }

            throw new gcException('Invalid PO Line ID ['+params.poLineId+'].');

        }else{

            if(params.productCode == null || params.productCode.isWhitespace()){
                throw new gcException('The Product Code may not be blank.');
            }

            for(AcctSeedERP__Purchase_Order_Line__c polx : [
                SELECT Id, 
                        Name,
                        AcctSeedERP__GL_Account_Variable_1__c,
                        AcctSeedERP__GL_Account_Variable_2__c,
                        AcctSeedERP__GL_Account_Variable_3__c,
                        AcctSeedERP__GL_Account_Variable_4__c,
                        AcctSeedERP__Product__c,
                        AcctSeedERP__Product__r.AcctSeed__Inventory_Product__c,
                        AcctSeedERP__Product__r.Case_Count__c,
                        AcctSeedERP__Product__r.GTIN_1__c,
                        AcctSeedERP__Product__r.GTIN_2__c,
                        AcctSeedERP__Project__c,
                        AcctSeedERP__Project_Task__c,
                        AcctSeedERP__Purchase_Order__c,
                        AcctSeedERP__Quantity__c,
                        AcctSeedERP__Quantity_Received__c,
                        AcctSeedERP__Quantity_Unreceived__c,
                        AcctSeedERP__Total__c,
                        AcctSeedERP__Unit_Price__c 
                FROM AcctSeedERP__Purchase_Order_Line__c 
                WHERE AcctSeedERP__Purchase_Order__c = :params.poId 
                  AND (AcctSeedERP__Product__r.GTIN_1__c = :params.productCode OR AcctSeedERP__Product__r.GTIN_2__c = :params.productCode OR AcctSeedERP__Product__r.ProductCode = :params.productCode)
                LIMIT 1
            ]){
                if(!polx.AcctSeedERP__Product__r.AcctSeed__Inventory_Product__c){
                    throw new gcException('The Product having Product Code or GTIN ['+params.productCode+'] is not an Inventory Product.');
                }
                params.poLine = polx;
                params.poLineId = polx.Id;
                return;
            }

            throw new gcException('A Product having Product Code or GTIN ['+params.productCode+'] is not on this Purchase Order.');
        }
    }

    public static void createPoMove(params params){
        
        retrievePoLine(params);

        if(params.qty == null)
            params.qty = params.poLine.AcctSeedERP__Product__r.Case_Count__c;
        if(params.qty == null)
            params.qty = 1;
        if(params.qty == null || params.qty <= 0)
            throw new gcException('The Quantity must be postive.');
        
        validateLocation(params.locId);

        Id balanceId = gcInventoryBalance.findOrCreateBalance(
            params.poLine.AcctSeedERP__Product__c, 
            params.locId);
        if(balanceId == null)
            throw new gcException('Error identifying balanceId');

        AcctSeedERP__Purchase_Order_Inventory_Movement__c move = new AcctSeedERP__Purchase_Order_Inventory_Movement__c(
            AcctSeedERP__GL_Account_Variable_1__c = params.poLine.AcctSeedERP__GL_Account_Variable_1__c,
            AcctSeedERP__GL_Account_Variable_2__c = params.poLine.AcctSeedERP__GL_Account_Variable_2__c,
            AcctSeedERP__GL_Account_Variable_3__c = params.poLine.AcctSeedERP__GL_Account_Variable_3__c,
            AcctSeedERP__GL_Account_Variable_4__c = params.poLine.AcctSeedERP__GL_Account_Variable_4__c,
            AcctSeedERP__Inventory_Balance__c = balanceId,
            AcctSeedERP__Movement_Date__c = date.Today(),
            AcctSeedERP__Project__c = params.poLine.AcctSeedERP__Project__c,
            AcctSeedERP__Project_Task__c = params.poLine.AcctSeedERP__Project_Task__c,
            AcctSeedERP__Purchase_Order_Line__c = params.poLine.Id,
            AcctSeedERP__Quantity__c = params.qty,
            AcctSeedERP__Serial_Number__c = params.serialNumber
        );

        try{
            insert move;
        }catch(exception e){
            throw new gcException('Error when inserting PO Movement: '+e.getMessage());
        }
    }

    public static void validateLocation(Id locId){

        if(locId == null)
            throw new gcException('Process Error:  locId may not be blank');
    
        for(AcctSeedERP__Location__c loc : [
            SELECT Id,
                    AcctSeedERP__Warehouse__c 
            FROM AcctSeedERP__Location__c 
            WHERE Id =:locId
        ]){
            return;
        }
        throw new gcException('Invalid Location ID ['+locId+'].');
    }

    @AuraEnabled 
    public static list<AcctSeedERP__Purchase_Order_Line__c> retrievePoLines(Id poId){
        return [
            SELECT Id, 
                    Name,
                    AcctSeedERP__GL_Account_Variable_1__c,
                    AcctSeedERP__GL_Account_Variable_2__c,
                    AcctSeedERP__GL_Account_Variable_3__c,
                    AcctSeedERP__GL_Account_Variable_4__c,
                    AcctSeedERP__Product__c,
                    AcctSeedERP__Product__r.Id,
                    AcctSeedERP__Product__r.GTIN_1__c,
                    AcctSeedERP__Product__r.Name,
                    AcctSeedERP__Product__r.ProductCode,
                    AcctSeedERP__Quantity__c,
                    AcctSeedERP__Quantity_Received__c,
                    AcctSeedERP__Quantity_Unreceived__c,
                    AcctSeedERP__Total__c,
                    AcctSeedERP__Unit_Price__c 
            FROM AcctSeedERP__Purchase_Order_Line__c 
            WHERE AcctSeedERP__Purchase_Order__c = :poId
              AND AcctSeedERP__Product__r.AcctSeed__Inventory_Product__c = TRUE
            ORDER BY AcctSeedERP__Product__r.Name, Name
        ];
    }

    @AuraEnabled 
    public static list<AcctSeedERP__Purchase_Order_Inventory_Movement__c> retrievePoMoves(Id filterId){
        return [
            SELECT Id, 
                    Name,
                    AcctSeedERP__Quantity__c,
                    AcctSeedERP__Inventory_Balance__r.AcctSeedERP__Location__r.Name,
                    AcctSeedERP__Inventory_Balance__r.AcctSeedERP__Product__r.GTIN_1__c,
                    AcctSeedERP__Inventory_Balance__r.AcctSeedERP__Product__r.Name,
                    AcctSeedERP__Inventory_Balance__r.AcctSeedERP__Product__r.ProductCode,
                    AcctSeedERP__Movement_Date__c,
                    AcctSeedERP__Serial_Number__c,
                    AcctSeedERP__Unit_Cost__c 
            FROM AcctSeedERP__Purchase_Order_Inventory_Movement__c 
            WHERE AcctSeedERP__Purchase_Order_Line__c = :filterId
               OR AcctSeedERP__Purchase_Order_Line__r.AcctSeedERP__Purchase_Order__c = :filterId
            ORDER BY CreatedDate DESC, Id DESC
        ];
    }

    @AuraEnabled 
    public static list<AcctSeedERP__Purchase_Order_Inventory_Movement__c> deleteChild(Id recordId, Id parentId){
        Database.Delete(recordId);
        return retrievePoMoves(parentId);
    }

    private static list<AcctSeedERP__Warehouse__c> retrieveWhsOptions(){

        list<AcctSeedERP__Warehouse__c> listWhs = new list<AcctSeedERP__Warehouse__c>();
        for(AcctSeedERP__Warehouse__c whs : [
            SELECT Id,
                    Name,
                    (SELECT Id FROM AcctSeedERP__Locations__r LIMIT 1)
            FROM AcctSeedERP__Warehouse__c
            ORDER BY Name
        ]){
            if(!whs.AcctSeedERP__Locations__r.isEmpty())
                listWhs.add(new AcctSeedERP__Warehouse__c(Id = whs.Id,Name = whs.Name));
        }
        return listWhs;
    }

    private static map<Id,list<AcctSeedERP__Location__c>> retrieveMapLocations(){
        map<Id,list<AcctSeedERP__Location__c>> mapLoc = new map<Id,list<AcctSeedERP__Location__c>>();
        for(AcctSeedERP__Location__c l : [
            SELECT Id, 
                    Name,
                    AcctSeedERP__Warehouse__c,
                    AcctSeedERP__Warehouse__r.Id,
                    AcctSeedERP__Warehouse__r.Name
            FROM AcctSeedERP__Location__c 
            ORDER BY AcctSeedERP__Warehouse__c,Name
        ]){
            list<AcctSeedERP__Location__c> listLoc = mapLoc.get(l.AcctSeedERP__Warehouse__c);
            if(listLoc == null){
                listLoc = new list<AcctSeedERP__Location__c>();
                mapLoc.put(l.AcctSeedERP__Warehouse__c,listLoc);
            }
            listLoc.add(new AcctSeedERP__Location__c(
                Id = l.Id,
                Name = l.Name
            ));
        }
        return mapLoc;
    }

    private static AcctSeedERP__Location__c retrieveLastLocation(Id poId){
        AcctSeedERP__Location__c location;
        Id userId = UserInfo.getUserId();
        for(AcctSeedERP__Purchase_Order_Inventory_Movement__c poim : [
            SELECT Id, 
                    AcctSeedERP__Inventory_Balance__c,
                    AcctSeedERP__Inventory_Balance__r.AcctSeedERP__Location__c,
                    AcctSeedERP__Inventory_Balance__r.AcctSeedERP__Location__r.Id,
                    AcctSeedERP__Inventory_Balance__r.AcctSeedERP__Location__r.Name,
                    AcctSeedERP__Inventory_Balance__r.AcctSeedERP__Location__r.AcctSeedERP__Warehouse__c
            FROM AcctSeedERP__Purchase_Order_Inventory_Movement__c
            WHERE AcctSeedERP__Purchase_Order_Line__r.AcctSeedERP__Purchase_Order__c = :poId
            ORDER BY CreatedDate DESC, Id DESC 
            LIMIT 1
        ]){
            location = poim.AcctSeedERP__Inventory_Balance__r.AcctSeedERP__Location__r;
        }

        if(location == null){
            for(AcctSeedERP__Location__c locx : [
                SELECT Id,
                        Name,
                        AcctSeedERP__Warehouse__c
                FROM AcctSeedERP__Location__c 
                WHERE AcctSeedERP__Warehouse__r.Name LIKE '%2%' 
                  AND Name LIKE '%Unfinished%'
                LIMIT 1
            ]){
                location = locx;
            }
            if(location == null)
                throw new gcException('A Location name containing [Unfinished] does not exist in Warehouse 2.');
        }

        return location;
    }

    @AuraEnabled 
    public static locationData retrieveLocationData(Id poId){
        locationData locationData = new locationData();
        locationData.lastLocation = retrieveLastLocation(poId);
        locationData.whsOptions = retrieveWhsOptions();
        locationData.mapLocations = retrieveMapLocations();

        return locationData;
    }

    public class locationData{
        @AuraEnabled
        public AcctSeedERP__Location__c lastLocation{get;set;}
        @AuraEnabled
        public list<AcctSeedERP__Warehouse__c> whsOptions{get;set;}
        @AuraEnabled
        public map<Id,list<AcctSeedERP__Location__c>> mapLocations{get;set;}
    }

